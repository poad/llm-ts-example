{
  "app": "pnpm dlx tsx bin/cdk.ts",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules",
      "test"
    ]
  },
  "requireApproval": "never",
  "versionReporting": false,
  "pathMetadata": false,
  "context": {
    "default": {
      "stackName": "llm-ts-example",
      "bucketName": "llm-ts-example",
      "cloudfront": {
        "comment": "Example LLM Lambda"
      }
    }
  }
}
